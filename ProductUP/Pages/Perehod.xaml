<Page x:Class="ProductUP.Pages.Perehod"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ProductUP.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Perehod">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Margin="10" x:Name="ProductBtn" Style="{StaticResource Button}" Click="ProductBtn_Click">Список продуктов</Button>
            <Button Margin="10" x:Name="OrderBtn" Style="{StaticResource Button}">Список заказов</Button>
            <Button Margin="10" x:Name="OrderOfBtn" Style="{StaticResource Button}">Оформление заказа</Button>
        </StackPanel>
        
    </Grid>
</Page>
